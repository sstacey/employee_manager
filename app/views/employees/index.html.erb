<div class="row mb-3">
  <div class="col-md-6">
    <h1>Employees</h1>
  </div>
  <div class="col-md-6 text-end">
    <%= button_to 'New Employee', new_employee_path, class: "btn btn-outline-primary", method: :get %>
  </div>
</div>

<%= render 'employees/search_form' %>
<%= render 'employees/table' %>
<div class="digg_pagination">
  <%= will_paginate @employees %>
</div>
<%= "#{@employees.count} employees found." %>
