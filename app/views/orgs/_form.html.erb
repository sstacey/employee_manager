<h1>New Org</h1>

<%= form_with model: org do |form| %>
  <div class="">
    <%= form.label :code %>
    <%= form.text_field :code %>
    <% org.errors.full_messages_for(:code).each do |message| %>
      <div class=""><%= message %></div>
    <% end %>
  </div>
  <div class="">
    <%= form.label :name %>
    <%= form.text_field :name %>
    <% org.errors.full_messages_for(:name).each do |message| %>
      <div class=""><%= message %></div>
    <% end %>
  </div>
  <div class="">
    <%= form.submit %>
  </div>
<% end %>